        "printWidth": 80,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "semi": true,
        "useTabs": false
    },
    "nodemonConfig": {
        "watch": [
            "src/**/*.ts",
            "src/**/*.json",
            "public/**/*"
        ],
        "exec": "ts-node",
        "quiet": true,
        "events": {
            "start": "clear || cls",
            "restart": "clear || cls"
        }
