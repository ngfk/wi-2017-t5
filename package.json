    "author": "",
    "license": "MIT",
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngfk/wi-2017-t5.git"
    },
    "bugs": {
        "url": "https://github.com/ngfk/wi-2017-t5/issues"
    },
    "homepage": "https://github.com/ngfk/wi-2017-t5#readme",
    "dependencies": {
        "body-parser": "^1.18.2",
        "cfenv": "^1.0.4",
        "cors": "^2.8.4",
        "express": "^4.16.2",
        "express-jwt": "^5.3.0",
        "jsonwebtoken": "^8.1.0",
        "multer": "^1.3.0",
        "uuid": "^3.1.0",
        "watson-developer-cloud": "^3.0.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.3",
        "@types/express": "^4.0.39",
        "@types/express-jwt": "0.0.38",
        "@types/jsonwebtoken": "^7.2.4",
        "@types/multer": "^1.3.6",
